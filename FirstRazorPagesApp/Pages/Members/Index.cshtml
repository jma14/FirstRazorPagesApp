@page
@model FirstRazorPagesApp.Pages.Members.IndexModel
@{
    ViewData["Title"] = "Members";
}

<h2>Members</h2>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Title</th>
            <th>Email</th>
            <th>Edit</th>
        </tr>
    </thead>
    @foreach (var member in @Model._Members)
    {
    <tr>
        <td>@member.FirstName</td>
        <td>@member.LastName</td>
        <td>@member.Title</td>
        <td>@member.Email</td>
        <td><a asp-page="Admin/Index" asp-route-id="@member.ID">Edit</a></td>
    </tr>
    }
</table>

